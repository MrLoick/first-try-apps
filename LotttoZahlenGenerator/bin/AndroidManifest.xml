            android:name=".LottoZahlen"
            android:label="@string/title_activity_lotto_zahlen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>